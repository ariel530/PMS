    <script>
        var projectFileId = "";

        function SetDeleteId(projectfileid) {
            projectFileId = projectfileid;
            document.getElementById('modal-delete-para').innerHTML = "Do you really want to delete project file. <span class='text-danger'> </span> ? This process cannot be undone. ";
        }

        function SureDeletion() {
            location.href = "/Project/DeleteProjectFileDetailData?id=" + projectFileId+"&projectId="+@Model.ProjectInfoData.Id;
        }

        function ViewFileData(projectfileid) {

            var preloaderText = '<div class="overlay d-flex justify-content-center align-items-center" id="overlay_preloader_inner">'
                + '<i class="fas fa-2x fa-sync fa-spin" ></i >'
                + '</div>';
            _("overlay_preloader").insertAdjacentHTML('afterend',preloaderText);
                   GetAndViewSpecificFileData(projectfileid);

        }

        function GetAndViewSpecificFileData(projectfileid) {
            $.ajax({
                type: "POST",
                url: '@Url.Action("GetSpecificProjectFileDetail")',
                data: '{id: ' + JSON.stringify(projectfileid) +  '}',
                dataType: "json",
                contentType: "application/json; charset=utf-8",
                success: function (responseData) {

                    if (responseData != null && responseData.ProjectFilesDetails != null) {
                        var resultList = JSON.parse(responseData.ProjectFilesDetails);
                      //  alert(resultList.FileName);
                        _("_filetitle").innerHTML = resultList.Title;
                        _("_filename").innerHTML = resultList.FileName;
                        _("_addedon").innerHTML = resultList.AddedOn;
                        _("_addedby").innerHTML = resultList.AddedBy;
                        _("overlay_preloader_inner").remove();


                        var buttonsSet = '<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>'
                            + '<a href="/' + resultList.FilePath + '" download="' + resultList.FileName + '" class="btn btn-success">Download</a>';
                        var myNode = _("viewfiledetailbuttons");
                        var fc = myNode.firstChild;

                        while (fc) {
                            myNode.removeChild(fc);
                            fc = myNode.firstChild;
                        }
                        _("viewfiledetailbuttons").insertAdjacentHTML('beforeend', buttonsSet);



                    }
                    else {

                        return false;
                    }
                },
                error: function (responseData) {

                    return false;
                }
            });

        }

    </script>

